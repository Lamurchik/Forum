
@model List<Post>
@{
    ViewData["Title"] = "Home Page";
}

<div class="post-list">
    @if(Model !=null)
    {
        @foreach (var post in Model)
        {
            <div class="post-summary">
                <h2 class="post-title"><a href="/Post/@post.PostId">@post.PostTitle</a></h2>
                <h3 class="post-subtitle">@post.PostSubtitle</h3>
                <p class="post-content">@post.PostBody</p>

            </div>
        }
    } 
</div>
